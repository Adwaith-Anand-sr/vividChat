      },
      "web": {
         "bundler": "metro",
         "output": "static",
         "favicon": "./assets/images/VIVID_CHAT_LOGO.png"
      },
      "plugins": [
         "expo-router"
      ],
      "experiments": {
         "typedRoutes": true
      },
      "extra": {
